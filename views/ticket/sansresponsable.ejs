<!DOCTYPE html>
<html lang="fr">
    <%- include('../shared/head', {css: ['details', 'navbar']}) %>
    <body>
        <%- include('../shared/navbar', {}) %>
        <%if (tickets && tickets.length>0){%>
        <section class="liste-tickets-sans-responsable">
            <%tickets.forEach(function(ticket){%>
            <p><%=ticket.title%> - <a href="/ticket/<%=ticket._id%>">Voir</a> - <a href="/ticket/<%=ticket._id%>/edit">Modifier</a></p>
            <%});%>
        </section>
        <%}else{%>
        <p>Aucun ticket sans responsable en base.</p>
        <%};%>
    </body>
</html>
